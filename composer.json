{
	"name": "webdna/twig-display",
	"description": "Use twig to render what you need.",
	"type": "craft-plugin",
	"version": "1.0.4",
	"keywords": [
		"craft",
		"cms",
		"craftcms",
		"craft-plugin",
		"twig display"
	],
	"support": {
		"docs": "https://github.com/webdna/twig-display/blob/master/README.md",
		"issues": "https://github.com/webdna/twig-display/issues"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "WebDNA",
			"homepage": "https://webdna.co.uk"
		}
	],
	"require": {
		"craftcms/cms": "^3.0.0-RC1"
	},
	"autoload": {
		"psr-4": {
			"webdna\\twigdisplay\\": "src/"
		}
	},
	"extra": {
		"name": "Twig Display",
		"handle": "twig-display",
		"hasCpSettings": false,
		"hasCpSection": false,
		"changelogUrl": "https://raw.githubusercontent.com/webdna/twig-display/master/CHANGELOG.md",
		"class": "webdna\\twigdisplay\\TwigDisplay"
	}
}
